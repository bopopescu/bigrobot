# Topology description is in YAML format - see http://en.wikipedia.org/wiki/YAML
#
# c<n>: Controllers
# s<n>: Switches
# h<n>: Hosts (including bare metal)
# mn  : Mininet server

c1:
  ip: 10.192.5.51

mn:
  ip: 10.192.7.39

  type: 't6'  # required: 't6' or 'basic'

  # T6 Mininet topology
  topology: --num-spine 0 --num-rack 1 --num-bare-metal 2 --num-hypervisor 0

  # Basic Mininet topology
  #topology: tree,4,2
