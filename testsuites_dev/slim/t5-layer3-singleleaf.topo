# Topology description is in YAML format - see http://en.wikipedia.org/wiki/YAML
#
# c<n>: Controllers
# s<n>: Switches
# h<n>: Hosts (including bare metal)
# mn  : Mininet server

c1:
  ip: 10.193.0.54
  #ip: 10.192.5.56

mn:
  ip: 10.193.0.171
  #ip: 10.192.7.213

  type: 't6'  # required: 't6' or 'basic'

  # Mininet topology
  topology: --num-spine 0 --num-rack 1 --num-bare-metal 1 --num-hypervisor 0
  
