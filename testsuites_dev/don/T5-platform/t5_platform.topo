# Topology description is in YAML format - see http://en.wikipedia.org/wiki/YAML
#
# c<n>: Controllers
# s<n>: Switches
# h<n>: Hosts (including bare metal)
# mn  : Mininet server

c1:
  ip: 192.168.250.100
c2:
  ip: 192.168.250.101
#mn:
#  ip: 192.168.250.135

  type: 't6'  # required: 't6' or 'basic'

  # Mininet topology
  topology: --num-spine 2 --num-rack 3 --num-bare-metal 2 --num-hypervisor 0 

